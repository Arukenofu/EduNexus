{
  "name": "electron-nuxt3",
  "author": "EternalC0DER",
  "email": "s.eternalcoder@gmail.com",
  "description": "Nuxt3 + Electron & More",
  "version": "1.0.0",
  "private": true,
  "main": ".output/electron/background.js",
  "scripts": {
    "dev": "nuxt dev -o",
    "build": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && electron-builder install-app-deps",
    "transpile-electron": "tsc -p ./electron --outDir .output/electron",
    "dev:electron": "NODE_ENV=development concurrently --kill-others \"nuxt dev\" \"tsc-watch -p ./electron --outDir .output/electron --onSuccess 'electron ./.output/electron/background.js'\"",
    "dev:electron:win": "set NODE_ENV=development& concurrently --kill-others \"nuxt dev\" \"tsc-watch -p ./electron --outDir .output/electron --onSuccess run.electron\"",
    "build:electron": "npm run build && npm run transpile-electron && node build.js"
  },
  "devDependencies": {
    "@nuxt/image": "^1.4.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "concurrently": "^8.0.1",
    "electron": "^23.2.1",
    "electron-builder": "^23.6.0",
    "nuxt": "^3.3.3",
    "nuxt-monaco-editor": "^1.2.9",
    "nuxt-tiptap-editor": "^1.1.2",
    "sass": "^1.60.0",
    "sass-loader": "^13.2.2",
    "tsc-watch": "^6.0.0",
    "typescript": "^5.0.3",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "@aarkue/tiptap-math-extension": "^1.2.2",
    "@nuxtjs/google-fonts": "^3.1.3",
    "@pinia/nuxt": "^0.4.7",
    "@tiptap/extension-code-block": "^2.3.2",
    "@tiptap/extension-code-block-lowlight": "^2.3.2",
    "@tiptap/extension-text-align": "^2.3.2",
    "@vueuse/nuxt": "^10.9.0",
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "electron-log": "^5.0.0-beta.16",
    "electron-updater": "^5.3.0",
    "express": "^4.18.2",
    "gsap": "^3.12.5",
    "lowlight": "^3.1.0",
    "nuxt-icon": "^0.6.9",
    "tiptap-extension-margin": "^1.0.0"
  }
}
